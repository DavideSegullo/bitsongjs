{
	"name": "@bitsonjs/api",
	"version": "1.0.0-testnet.1",
	"author": "Angelo Recca <angelorc@users.noreply.github.com>",
	"description": "Javascript API library for NodeJS and Web browsers to interact with the BitSong Blockchain.",
	"license": "Apache-2.0",
	"main": "lib",
	"publishConfig": {
		"access": "public"
	},
	"repository": "https://github.com/bitsongofficial/bitsongjs/tree/testnet/packages/api",
	"dependencies": {
		"@cosmjs/stargate": "^0.27.0"
	},
	"devDependencies": {
		"@regen-network/ts-proto": "1.105.2-1"
	},
	"scripts": {
		"preget-proto": "rimraf proto",
		"get-proto": "CREF=v0.45.6 IBCREF=v2.0.0 BREF=v0.11.0 ./scripts/get-proto.sh",
		"define-proto": "./scripts/define-proto.sh",
		"postdefine-proto": "prettier --write \"src/codec/**/*.ts\"",
		"proto": "yarn preget-proto && yarn get-proto",
		"build": "rimraf lib && tsc",
		"gen": "rimraf src/generated && mkdir src/generated && ./scripts/protocgen.sh",
		"test": "jest"
	}
}