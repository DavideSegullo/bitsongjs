{
  "name": "@bitsongjs/sdk",
  "version": "v1.0.0-dev-14",
  "license": "Apache-2.0",
  "description": "Javascript SDK library for NodeJS and Web browsers to interact with the BitSong Blockchain.",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/",
    "*.md",
    "!*.spec.*",
    "!**/testdata/"
  ],
  "author": "Angelo Recca <angelorc@users.noreply.github.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/bitsongofficial/bitsongjs/tree/dev/packages/sdk"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "tags": [
    "bitsong",
    "sdk",
    "typescript",
    "blockchain",
    "rpc",
    "cosmos",
    "tendermint",
    "javascript"
  ],
  "scripts": {
    "test-node": "yarn node jasmine-testrunner.js",
    "test-firefox": "yarn pack-web && karma start --single-run --browsers Firefox",
    "test-chrome": "yarn pack-web && karma start --single-run --browsers ChromeHeadless",
    "test": "yarn build-or-skip && yarn test-node",
    "build-or-skip": "[ -n \"$SKIP_BUILD\" ] || yarn build",
    "build": "rm -rf ./build && tsc",
    "bump": "npm version",
    "preget-proto": "rm -r proto",
    "get-proto": "CREF=v0.45.6 IBCREF=v2.0.0 BREF=v0.11.0 ./scripts/get-proto.sh",
    "define-proto": "./scripts/define-proto.sh",
    "define-proto2": "cross-env NODE_ENV=development babel-node ./scripts/gen-proto.js",
    "postdefine-proto": "prettier --write \"src/codec/**/*.ts\""
  },
  "dependencies": {
    "@babel/runtime": "^7.18.9",
    "@cosmjs/crypto": "^0.28.11",
    "@cosmjs/encoding": "^0.28.11",
    "@cosmjs/math": "^0.28.11",
    "@cosmjs/proto-signing": "^0.28.11",
    "@cosmjs/stargate": "^0.28.11",
    "@cosmjs/tendermint-rpc": "^0.28.11",
    "@cosmjs/utils": "^0.28.11",
    "@types/crypto-js": "^4.0.1",
    "@types/uuid": "^8.3.0",
    "crypto-browserify": "^3.12.0",
    "crypto-js": "^4.0.0",
    "long": "^4.0.0",
    "protobufjs": "^6.11.3",
    "semver": "^7.3.5",
    "ts-proto": "^1.118.0",
    "uuid": "^8.3.2"
  },
  "optionalDependencies": {},
  "devDependencies": {
    "@babel/cli": "7.18.9",
    "@babel/core": "7.18.9",
    "@babel/eslint-parser": "^7.18.9",
    "@babel/generator": "7.18.9",
    "@babel/node": "^7.18.9",
    "@babel/parser": "^7.18.9",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-proposal-export-default-from": "7.18.9",
    "@babel/plugin-proposal-object-rest-spread": "7.18.9",
    "@babel/plugin-transform-runtime": "7.18.9",
    "@babel/preset-env": "7.18.9",
    "@babel/preset-typescript": "^7.17.12",
    "@babel/traverse": "7.18.9",
    "@babel/types": "7.18.9",
    "@types/jasmine": "^4",
    "@types/karma-firefox-launcher": "^2",
    "@types/karma-jasmine": "^4",
    "@types/karma-jasmine-html-reporter": "^1",
    "@types/semver": "^7.3.8",
    "@types/long": "^4.0.1",
    "@types/node": "^15.0.1",
    "axios": "^0.21.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.5",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "esm": "^3.2.25",
    "jasmine": "^4",
    "jasmine-spec-reporter": "^6",
    "karma": "^6.3.14",
    "karma-chrome-launcher": "^3.1.0",
    "karma-firefox-launcher": "^2.1.0",
    "karma-jasmine": "^5",
    "karma-jasmine-html-reporter": "^1.5.4",
    "gts": "^3.1.0",
    "prettier": "^2.7.0",
    "rimraf": "^3.0.2",
    "ts-node": "^8",
    "typedoc": "^0.22",
    "typescript": "~4.6",
    "webpack": "^5.32.0",
    "webpack-cli": "^4.6.0"
  }
}
